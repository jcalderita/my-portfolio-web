{
  "name": "my-portfolio-web",
  "version": "0.1.1",
  "description": "Portfolio/Cv web.",
  "main": "index.html",
  "scripts": {
    "sass": "sass --watch scss/main.scss:css/main.css",
    "live": "live-server",
    "start": "start npm run sass && start npm run live",
    "dev": "parcel serve index.html",
    "build": "parcel build index.html",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcalderita/my-portfolio-web.git"
  },
  "keywords": [
    "portfolio",
    "cv"
  ],
  "author": "jcalderita",
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/jcalderita/my-portfolio-web/issues"
  },
  "homepage": "https://github.com/jcalderita/my-portfolio-web#readme",
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "parcel": "^2.0.0-beta.2",
    "parcel-plugin-lazy": "^0.0.4",
    "postcss": "^8.2.8"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "overrideBrowserslist": [
          "> 1%",
          "last 4 versions",
          "ie >= 9"
        ]
      }
    }
  }
}
